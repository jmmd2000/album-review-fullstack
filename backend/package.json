{
  "name": "@album-reviews/backend",
  "version": "1.0.0",
  "main": "src/index.ts",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon --watch src --legacy-watch --exec tsx src/index.ts",
    "build": "tsc",
    "test": "jest --runInBand",
    "test:album": "jest album.test.ts --detectOpenHandles",
    "test:artist": "jest artist.test.ts --detectOpenHandles",
    "test:spotify": "jest spotify.test.ts --detectOpenHandles",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio --verbose",
    "db:seed": "tsx ./src/db/seed.ts",
    "db:wipe": "tsx ./src/db/wipe.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@album-reviews/shared": "file:../shared",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "drizzle-kit": "^0.30.4",
    "drizzle-orm": "^0.39.3",
    "express": "^4.21.2",
    "extract-colors": "^4.1.1",
    "get-pixels": "^3.3.3",
    "node-mocks-http": "^1.16.2",
    "pg": "^8.13.3",
    "puppeteer": "^24.6.1",
    "slugify": "^1.6.6",
    "socket.io": "^4.8.1",
    "tsconfig-paths": "^4.2.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/get-pixels": "^3.3.4",
    "@types/node": "^22.13.4",
    "@types/pg": "^8.11.11",
    "@types/socket.io": "^3.0.1",
    "@types/socket.io-client": "^1.4.36",
    "@types/supertest": "^6.0.2",
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "nodemon": "^3.1.9",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  }
}
